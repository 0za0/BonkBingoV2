<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			xmlns:local="using:BingoOnline.Views"
			 mc:Ignorable="d" d:DesignWidth="625" d:DesignHeight="625"
             x:Class="BingoOnline.Views.BingoFieldView">


	<ItemsControl Items="{Binding Buttons}">
		<ItemsControl.ItemsPanel>
			<ItemsPanelTemplate>
				<UniformGrid Rows="5" Columns="5">
					
				</UniformGrid>
			</ItemsPanelTemplate>
		</ItemsControl.ItemsPanel>
		
		<ItemsControl.ItemTemplate>
			<DataTemplate>
				<local:BingoButton DataContext="{Binding }"/>
			</DataTemplate>
		</ItemsControl.ItemTemplate>
	</ItemsControl>





</UserControl>
